	  <tr id="product_presentation_<%= product_presentation.id %>">
        <td><%= product_presentation.name %></td>
        <td><%= product_presentation.description %></td>
        <td><%= product_presentation.product.name %></td>
        <td><%= product_presentation.unit_type.abbreviation unless product_presentation.unit_type.nil? %></td>
        <td><%= product_presentation.quantity %></td>
        <td><%= link_to 'Show', product_presentation %></td>
        <td><%= link_to 'Destroy', product_presentation, method: :delete, remote: true, data: { confirm: 'Are you sure?' } %></td>
      </tr>