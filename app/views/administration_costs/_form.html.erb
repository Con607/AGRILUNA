<%= simple_form_for(@administration_cost) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :greenhouse %>
    <%= f.input :event_date %>
    <%= f.input :concept %>
    <%= f.input :display %>
    <%= f.input :unit_price %>
    <%= f.input :quantity %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
