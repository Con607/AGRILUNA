
	<tr id="application_product_<%= application_product.id %>">
      <td><%= application_product.name %></td>
      <td><%= application_product.description %></td>
      <td><%= application_product.unit_type.name unless application_product.unit_type.nil? %></td>
      <td><%= application_product.quantity_available %></td>
      <td><%= application_product.unit_cost %></td>
      <td><%= application_product.total_cost %></td>
      <td><%= link_to 'Show', application_product %></td>
      <td><%= link_to 'Destroy', application_product, method: :delete, remote: true, data: { confirm: 'Are you sure?' } %></td>
    </tr>