	  <tr id="product_<%= product.id %>">
        <td><%= product.name %></td>
        <td><%= product.description %></td>
        <td><%= product.product_variety_ids %></td>
        <td><%= product.product_presentation_ids %></td>
        <td><%= product.product_quality_ids %></td>
        <td><%= product.harvest_ids %></td>
        <td><%= product.selection_ids %></td>
        <td><%= product.sale_ids %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Destroy', product, method: :delete, remote: true, data: { confirm: 'Are you sure?' } %></td>
      </tr>